    "lint-staged": "^8.1.1",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "ts-lint": "^4.5.1",
    "tsconfig-paths": "^3.7.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }

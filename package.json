    "@types/node-localstorage": "^1.3.0",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack": "^4.4.24",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-styled-components": "^1.10.0",
